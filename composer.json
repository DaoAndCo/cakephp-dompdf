{
    "name": "your-name-here/Dompdf",
    "description": "Dompdf plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "dompdf/dompdf": "0.7.x@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Dompdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dompdf\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
