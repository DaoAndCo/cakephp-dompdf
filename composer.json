{
    "name": "daoandco/cakephp-dompdf",
    "description": "Dompdf plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "pdf",
        "dompdf",
        "html",
        "css"
    ],
    "homepage": "https://github.com/DaoAndCo/cakephp-dompdf",
    "license": "MIT",
    "authors": [
        {
            "name": "Flavien Beninca",
            "email": "contact@flavienbeninca.fr",
            "homepage": "http://www.flavienbeninca.fr",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "dompdf/dompdf": "0.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Dompdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dompdf\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support": {
        "email": "contact+dompdf@flavienbeninca.fr",
        "issues": "https://github.com/DaoAndCo/cakephp-dompdf/issues",
        "source": "https://github.com/DaoAndCo/cakephp-dompdf"
    }
}
